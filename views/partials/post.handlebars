<article class="post">
	<p class="post-title">
		{{title}}
	</p>
	<p class="post-author">
		Posted By: {{author}}
	</p>
	<p class="post-category">
		Category: {{category}}
	</p>
	<p class="post-date">
		Post Date: {{date}}
	</p>
	<p class="post-location">
		Location: {{location}}
	</p>
	<p class="post-price">
		{{price}}
	</p>
	<p class="post-phone">
		Phone: {{phone}}
	</p>
	<p class="post-email">
		E-mail: {{email}}
	</p>
	
	<br>
	<p>Images:</p>
	<div class="img-container">
		<a href="{{{image1}}}"><img src="{{{image1}}}"></a>
	</div>
	<div class="img-container">
		<a href="{{{image2}}}"><img src="{{{image2}}}"></a>
	</div>
	<div class="img-container">
		<a href="{{{image3}}}"><img src="{{{image3}}}"></a>
	</div>
</article>